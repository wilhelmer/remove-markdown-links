{
	"name": "remove-markdown-links",
	"displayName": "Remove Markdown Links",
	"description": "Removes Markdown links from selected text.",
	"version": "0.0.3",
	"publisher": "wilhelmer",
	"repository": "https://github.com/wilhelmer/remove-markdown-links.git",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:remove-markdown-links.removeLinks"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "remove-markdown-links.removeLinks",
				"title": "Remove Markdown links"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
